{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wp-dos-dialog/ms-dos-dialog",
	"version": "1.0.0",
	"title": "MS DOS Dialog",
	"category": "text",
	"icon": "desktop",
	"description": "A Gutenberg block that displays your text in the form of an old MS DOS dialog box.",
	"attributes": {
		"window_title": {
			"type": "string",
			"source": "text",
			"selector": ".wp-dos-dialog-title"
		},
		"message": {
			"type": "string",
			"source": "html",
			"selector": ".wp-dos-dialog-message"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "wp-dos-dialog",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
